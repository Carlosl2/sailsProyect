<div class="row">
    <div class="col-md-12">
        
        <% flash.get('mensaje').forEach(function (msg) { %>
        <div class="alert alert-info text-center" role="alert">
            <%=msg%>
        </div>
        <% }) %>

        <h2>Lista de Deseo</h2>

        <table class="table">
            <thead>
                <tr>
                    
                    <th scope="col">Foto</th>
                </tr>
            </thead>
            <tbody>

                <% elementosL.forEach( elemento => { %>
                <tr>  
                  
                    <td><img src="images/<%=elemento.foto.contenido%>" alt="" class="wf-10"  > <a class="btn btn-light mx-2"  href="/lista-deseo-eliminar/<%=elemento.foto.id%>">Eliminar</a></td>
                </tr>
            
                <% }) %>


            </tbody>
        </table>
      
    </div>
</div>